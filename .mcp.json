{
  "mcpServers": {
    "memory": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "@modelcontextprotocol/server-memory"
      ],
      "env": {}
    },
    "github": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_TOKEN}",
        "GITHUB_REPOSITORY": "dprograma/classified_ad"
      }
    },
    "filesystem": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "@modelcontextprotocol/server-filesystem",
        "/Users/admin/projects/classified_ad"
      ],
      "env": {}
    },
    "shell": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "@mako10k/mcp-shell-server"
      ],
      "env": {}
    },
    "context7": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp", "--api-key", "ctx7sk-968f1c34-d45c-4b90-9642-09cbfd47882c"]
    },
    "mysql": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "@liangshanli/mcp-server-mysql"
      ],
      "env": {
        "MYSQL_HOST": "localhost",
        "MYSQL_PORT": "3306",
        "MYSQL_USER": "root",
        "MYSQL_PASSWORD": "",
        "MYSQL_DATABASE": "eunixmacdb"
      }
    },
    "browser-tools": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "@eqiz/browser-tools-mcp-enhanced"
      ],
      "env": {}
    }
  }
}